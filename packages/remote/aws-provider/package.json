{
  "name": "@wvb/remote-aws-provider",
  "version": "0.1.0",
  "description": "Webview Bundle remote provider for AWS",
  "homepage": "https://github.com/webview-bundle/webview-bundle",
  "bugs": {
    "url": "https://github.com/webview-bundle/webview-bundle/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Seokju Na",
    "email": "seokju.me@gmail.com",
    "url": "https://github.com/seokju-na"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webview-bundle/webview-bundle",
    "directory": "packages/remote/aws-provider"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./origin-request": {
      "import": "./dist/origin-request/index.mjs",
      "require": "./dist/origin-request/index.cjs"
    },
    "./origin-response": {
      "import": "./dist/origin-response/index.mjs",
      "require": "./dist/origin-response/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3",
    "@types/aws-lambda": "^8.10.159",
    "aws-lambda": "^1.0.7",
    "hono": "^4.11.10"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "esbuild": "0.25.3",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
